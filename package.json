{
  "name": "vscode-github-opener",
  "displayName": "Github Opener",
  "description": "Quickly open the GitHub repository in your browser",
  "version": "1.5.0",
  "publisher": "caphefalumi",
  "license": "SEE LICENSE IN LICENSE.md",
  "galleryBanner": {
    "color": "#E9EBED",
    "theme": "light"
  },
  "homepage": "https://github.com/caphefalumi/vscode-github-opener/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/caphefalumi/vscode-github-opener.git"
  },
  "bugs": {
    "url": "https://github.com/caphefalumi/vscode-github-opener/issues"
  },
  "categories": [
    "Other"
  ],
  "engines": {
    "vscode": "^1.30.0"
  },
  "activationEvents": [
    "onCommand:extension.githubOpener"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.githubOpener",
        "title": "Open in GitHub"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "GitHub Opener extension configuration",
      "properties": {
        "githubOpener.gitHubDomain": {
          "type": "string",
          "default": "github.com",
          "description": "Configure a custom GitHub domain. Useful for GitHub Enterprise"
        }
      }
    },
    "keybindings": [
      {
        "command": "extension.githubOpener",
        "key": "ctrl+alt+g",
        "mac": "cmd+alt+g"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.githubOpener"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "bun run esbuild-base --minify",
    "esbuild-base": "esbuild ./src/extension.js --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node",
    "esbuild": "bun run esbuild-base --sourcemap",
    "esbuild-watch": "bun run esbuild-base --sourcemap --watch",
    "package": "bun x vsce package",
    "publish": "bun x vsce publish"
  },
  "devDependencies": {
    "esbuild": "^0.19.0"
  },
  "dependencies": {
    "find-parent-dir": "^0.3.0",
    "git-url-parse": "^11.1.1",
    "ini": "^1.3.5",
    "open": "6.0.0"
  }
}

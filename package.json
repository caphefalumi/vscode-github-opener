{
  "name": "vscode-open-in-github",
  "displayName": "Open in GitHub",
  "description": "Quickly open the GitHub repository in your browser",
  "icon": "images/icon_200.png",
  "version": "1.4.1",
  "publisher": "caphefalumi",
  "license": "SEE LICENSE IN LICENSE.md",
  "galleryBanner": {
    "color": "#E9EBED",
    "theme": "light"
  },
  "homepage": "https://github.com/caphefalumi/vscode-github-opener/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/caphefalumi/vscode-github-opener.git"
  },
  "bugs": {
    "url": "https://github.com/caphefalumi/vscode-github-opener/issues"
  },
  "categories": [
    "Other"
  ],
  "engines": {
    "vscode": "^1.30.0"
  },
  "activationEvents": [
    "onCommand:extension.openInGitHub"
  ],
  "main": "./src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.openInGitHub",
        "title": "Open in GitHub"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Open in GitHub extension configuration",
      "properties": {
        "openInGitHub.gitHubDomain": {
          "type": "string",
          "default": "github.com",
          "description": "Configure a custom GitHub domain. Useful for GitHub Enterprise"
        }
      }
    },
    "keybindings": [
      {
        "command": "extension.openInGitHub",
        "key": "ctrl+alt+g",
        "mac": "cmd+alt+g"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.openInGitHub"
        }
      ]
    }
  },
  "scripts": {
    "package": "bun x vsce package",
    "publish": "bun x vsce publish"
  },
  "devDependencies": {},
  "dependencies": {
    "find-parent-dir": "^0.3.0",
    "git-url-parse": "^11.1.1",
    "ini": "^1.3.5",
    "open": "6.0.0"
  }
}
